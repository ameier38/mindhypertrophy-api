{
  "name": "mindhypertrophy_api",
  "version": "1.0.0",
  "description": "API for mindhypertrophy.com blog.",
  "main": "dist/server",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src/server --presets es2015\"",
    "build": "babel src -s -D -d dist --presets es2015",
    "start": "node dist/server",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ameier38/mindhypertrophy-api.git"
  },
  "author": "Andrew Meier <info@mindhypertrophy.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.13.3",
    "cors": "^2.7.1",
    "debug": "^2.6.1",
    "express": "^4.13.3",
    "mongoose": "^4.8.5",
    "morgan": "^1.8.0",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/ameier38/mindhypertrophy-api.git/issues"
  },
  "homepage": "http://mindhypertrophy.com",
  "keywords": [
    "mongoose",
    "mongodb",
    "mindhypertrophy",
    "express",
    "es6"
  ]
}
